# Shared configuration
VITE_GOOGLE_CLIENT_ID=your-google-client-id.apps.googleusercontent.com

# API configuration
Auth__JwtIssuer=http://localhost:5143
Auth__JwtAudience=marinapp-web
Auth__JwtSigningKey=replace-with-strong-key
Auth__JwtExpirationMinutes=60
Cors__AllowedOrigins=http://localhost:5173
ConnectionStrings__Default=Host=your-db-host;Port=5432;Database=marinapp;Username=marinapp;Password=replace-with-strong-password;SSL Mode=Require;Trust Server Certificate=true
Storage__S3BucketName=your-s3-bucket-name
Storage__S3Region=us-east-1
Storage__S3Prefix=clipboard

# AWS credentials (API service uses the standard AWS SDK credentials chain)
AWS_ACCESS_KEY_ID=your-aws-access-key-id
AWS_SECRET_ACCESS_KEY=your-aws-secret-access-key
AWS_SESSION_TOKEN=optional-session-token
AWS_REGION=us-east-1

# Web configuration
VITE_API_BASE_URL=http://localhost:5143
